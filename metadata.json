{
  "name": "joshsouza/stagecoach",
  "version": "0.0.1",
  "author": "development@pureinsomnia.com",
  "summary": "Installs and configures StageCoach for Node.js application deployment",
  "license": "Apache-2.0",
  "source": "https://github.com/joshsouza/puppet-stagecoach.git",
  "project_page": "https://github.com/joshsouza/puppet-stagecoach",
  "issues_url": "https://github.com/joshsouza/puppet-stagecoach/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    },{
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
